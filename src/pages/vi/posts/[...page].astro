---
import type { GetStaticPaths } from 'astro'
import PostsArchive from '~/pages/_shared/PostsArchive.astro'
import { getPostArchivePaths } from '~/pages/_shared/postsArchive'

export const getStaticPaths = (async ({ paginate }) => {
  return getPostArchivePaths('vi', paginate)
}) satisfies GetStaticPaths

const { page } = Astro.props
---

<PostsArchive lang="vi" page={page} />
