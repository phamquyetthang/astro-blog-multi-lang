---
import type { GetStaticPaths } from 'astro'
import TagArchive from '~/pages/_shared/TagArchive.astro'
import { getTagArchivePaths } from '~/pages/_shared/tagPaths'

export const getStaticPaths = (async ({ paginate }) => {
  return getTagArchivePaths('vi', paginate)
}) satisfies GetStaticPaths

const { page, tagTitle } = Astro.props
---

<TagArchive lang="vi" page={page} tagTitle={tagTitle} />
