---
import type { GetStaticPaths } from 'astro'
import SeriesArchive from '~/pages/_shared/SeriesArchive.astro'
import { getSeriesPaths } from '~/pages/_shared/seriesPaths'

export const getStaticPaths = (async () => {
  return getSeriesPaths('vi')
}) satisfies GetStaticPaths

const { posts, seriesTitle } = Astro.props
---

<SeriesArchive lang="vi" posts={posts} seriesTitle={seriesTitle} />
